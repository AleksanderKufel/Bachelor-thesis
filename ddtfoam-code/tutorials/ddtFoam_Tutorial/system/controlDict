FoamFile
{
    version         2.1;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             12e-3;

deltaT              2e-7;

writeControl        adjustableRunTime;

writeInterval       0.05e-3;

purgeWrite          0;

writeFormat         ascii;

writePrecision      6;

writeCompression    uncompressed;

timeFormat          fixed;

timePrecision       9;

adjustTimeStep      yes;

maxCo               0.3;
maxDeltaT	    2e-5;

runTimeModifiable   yes;



