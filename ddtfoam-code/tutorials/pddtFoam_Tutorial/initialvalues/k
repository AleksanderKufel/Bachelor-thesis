FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          k;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions          [ 0 2 -2 0 0 0 0 ];

internalField       uniform 0.1;

boundaryField
{
    wand
    {
        type            compressible::kqRWallFunction;
	value		uniform 0.1;
    }
    sym1 {type symmetryPlane;}
    frontAndBack {empty;}

}


