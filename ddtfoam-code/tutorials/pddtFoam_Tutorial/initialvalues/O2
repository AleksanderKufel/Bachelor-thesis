FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wand
    {
        type            zeroGradient;
    }
    sym1 {type symmetryPlane;}
    frontAndBack {empty;}

}


