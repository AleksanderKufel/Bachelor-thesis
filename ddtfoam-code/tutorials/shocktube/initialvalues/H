FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      H;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    left           
    {
//	type		zeroGradient;

	type		mixed;
	refValue	0.0;
	valueFraction	0.05;
	refGradient	0.0;

    }
    right
    {
        type            fixedValue;
	value		uniform 0.0;
    }
    cycle
    {
	type		cyclic;
    }
    wand
    {
	type		zeroGradient;
    }

    empty           
    {
        type            empty;
    }
}


